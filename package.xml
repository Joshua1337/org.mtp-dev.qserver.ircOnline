<?xml version="1.0" encoding="UTF-8"?>
<package name="org.mtp-dev.qserver.ircOnline" xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com/XSD/package.xsd">
