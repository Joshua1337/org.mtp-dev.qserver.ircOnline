		<isunique>1</isunique>
		<version>1.0.0</version>
		<date>2012-12-22</date>
		<plugin>com.woltlab.wcf.data.page</plugin>
	</packageinformation>

	<authorinformation>
		<author><![CDATA[NurPech]]></author>
		<authorurl><![CDATA[http://mtp-dev.org]]></authorurl>
	</authorinformation>

	<requiredpackages>
